APP_NAME=DriveUp
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_URL=https://votre-domaine.epizy.com

APP_LOCALE=fr
APP_FALLBACK_LOCALE=fr
APP_FAKER_LOCALE=fr_FR

LOG_CHANNEL=stack
LOG_LEVEL=warning

# Base de données InfinityFree (MySQL)
DB_CONNECTION=mysql
DB_HOST=sqlXXX.epizy.com
DB_PORT=3306
DB_DATABASE=epiz_XXXXXXXX_garage
DB_USERNAME=epiz_XXXXXXXX
DB_PASSWORD=votre_mot_de_passe

# Sessions / Cache sans tables SQL
SESSION_DRIVER=file
SESSION_LIFETIME=120
CACHE_STORE=file
QUEUE_CONNECTION=sync

# Fichiers
FILESYSTEM_DISK=local

# Redis (non utilisé)
REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

# Mail (désactivé sur InfinityFree en SMTP sortant)
MAIL_MAILER=log
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"
